  * (k+1,k+1)    desc_A->nt-2                  0
  * (m1,k+1)     0(2)                  0
  * (m1,m1)      2*(MT-3)+ 0(desc_A->nt..MT-1) 0
  * So, it's better to have a locality with (m1,m1), as soon as MT > 4
  *
  */

  : data_A(m1,m1)

  /* A == data_A(k+1,k) */
  /* E == data_A(k+1,k+1) */
  /* H == data_A(m,k+1) */
  /* K == data_A(m1,k) */
  /* L == data_T(m1,k) */
  /* M == data_A(i,k+1) */
  /* Q == data_A(j,k+1) */
  /* U == data_A(k+1,k+1) */
  /* V == data_A(m1,k+1) */
  /* W == data_A(m1,m1) */
  /* X == data_A(m1,k) */
  /* Y == data_T(m1,k) */

  RW    U <- ((k+2)==m1) ? E zherfb(m1-2) 
          <- (m1>=(3+k)) ? U ztsmqr_corner(k, m1-1) 
