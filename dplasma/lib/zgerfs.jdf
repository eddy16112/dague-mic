zlacpy_21(m,n)
  /* Execution space */
  m = 0..(descB.mt-1)
  n = 0..(descB.nt-1)

  : Z(m,n)

  /* A == B(m,n) */
  /* B == Z(m,n) */
  /* E == Z(m1,n1) */
  /* F == Z(m2,n2) */
  /* J == Z(k1,n3) */
  /* M == Z(m3,n4) */
  /* V == Z(k3,n8) */

  READ  A <- A zlacpy_21_in_B0(m,n)

  RW    B <- Z(m,n)
          -> (descZ.nt>=(1+n)) & (descA.nt>=1) & (descZ.mt>=(1+m)) ? E zgemm_36(m, n, 0) 
          -> (n>=descZ.nt) & ((m+1)>=descZ.mt) ? Z(m,n)
          -> (!((n>=descZ.nt) & ((m+1)>=descZ.mt))) & (descZ.nt>=(n+1)) & (m>=descZ.mt) ? Z(m,n)
          -> (!((n>=descZ.nt) & ((m+1)>=descZ.mt))) & (!((descZ.nt>=(n+1)) & (m>=descZ.mt))) & (descZ.mt>=(2+m)) & (n>=descZ.nt) ? Z(m,n)
          -> (descZ.mt>=1) & (descZ.nt>=(n+1)) & (descZ.mt>=(m+1)) & (0>=descA.nt) & (m>=1) ? M zgemm_72(0, m, n) 
          -> (n>=descZ.nt) & (descA.nt>=(m+1)) & (descR.nt>=(n+1)) & (descR.mt>=1) ? V zgemm_154(0..(descR.mt-1), n, m) 
          -> (!((n>=descZ.nt) & (descA.nt>=(m+1)) & (descR.nt>=(n+1)) & (descR.mt>=1))) & (descZ.nt>=(1+n)) & (m>=descZ.mt) & (descA.nt>=(1+m)) & (descR.nt>=(n+1)) & (descR.mt>=1) ? V zgemm_154(0..(descR.mt-1), n, m) 
          -> (descZ.mt>=(m+1)) & (descZ.nt>=(n+1)) & (0>=descA.nt) ? F zlacpy_48(m, n) 
          -> (0==m) & (descZ.nt>=(n+1)) & (descZ.mt>=1) & (0>=descA.nt) ? J ztrsm_59(0, n) 

  /*
  Anti-dependencies:

  */

BODY

  int tempmm = ((m)==(descB.mt-1)) ? (descB.m-(m*descB.mb)) : descB.mb;
  int tempnn = ((n)==(descB.nt-1)) ? (descB.n-(n*descB.nb)) : descB.nb;
  int ldbm = BLKLDD( descB, m );
  int ldzm = BLKLDD( descZ, m );

  DRYRUN(

    CORE_zlacpy(PlasmaUpperLower, tempmm, tempnn, 
	A /* B(m,n) */, ldbm, B /* Z(m,n) */, 
	ldzm );  );

  printlog("thread %d CORE_zlacpy(%d, %d)\n"
           "\t(PlasmaUpperLower, tempmm, tempnn, B(%d,%d)[%p], ldbm, Z(%d,%d)[%p], ldzm)\n",
  context->th_id, m, n, m, n, A, m, n, B);

END

/*
 * Pseudo-task
 */
zlacpy_21_in_B0(m,n)
  m = 0..(descB.mt-1)
  n = 0..(descB.nt-1)

  : B(m,n)

  RW A <- B(m,n)
       -> A zlacpy_21(m,n)
BODY
/* nothing */
END



zgemm_36(m1,n1,k)
  /* Execution space */
  m1 = 0..(descZ.mt-1)
  n1 = 0..(descZ.nt-1)
  k = 0..(descA.nt-1)

  : Z(m1,n1)

  /* B == Z(m,n) */
  /* C == A(m1,k) */
  /* D == X(k,n1) */
  /* E == Z(m1,n1) */
  /* F == Z(m2,n2) */
  /* J == Z(k1,n3) */
  /* M == Z(m3,n4) */

  READ  C <- C zgemm_36_in_A0(m1,n1,k)

  READ  D <- D zgemm_36_in_X1(m1,n1,k)

  RW    E <- (0==k) & (descB.nt>=(n1+1)) & (descB.mt>=(1+m1)) ? B zlacpy_21(m1, n1) 
          <- (k>=1) ? E zgemm_36(m1, n1, k-1) 
          <- (0==k) & (m1>=descB.mt) ? Z(m1,n1)
          <- (!((0==k) & (m1>=descB.mt))) & (0==k) & (descB.mt>=(1+m1)) & (n1>=descB.nt) ? Z(m1,n1)
          -> (descA.nt>=(k+2)) ? E zgemm_36(m1, n1, k+1) 
          -> (descA.nt==(1+k)) & (m1>=1) ? M zgemm_72(0, m1, n1) 
          -> (descA.nt==(k+1)) ? F zlacpy_48(m1, n1) 
          -> (descA.nt==(1+k)) & (0==m1) ? J ztrsm_59(0, n1) 

  /*
  Anti-dependencies:

  */

BODY

  int tempmm = ((m1)==(descZ.mt-1)) ? (descZ.m-(m1*descZ.mb)) : descZ.mb;
  int tempnn = ((n1)==(descZ.nt-1)) ? (descZ.n-(n1*descZ.nb)) : descZ.nb;
  int tempkn = ((k)==(descA.nt-1)) ? (descA.n-(k*descA.nb)) : descA.nb;
  int ldam = BLKLDD( descA, m1 );
  int ldxk = BLKLDD( descX, k );
  int ldzm = BLKLDD( descZ, m1 );

  DRYRUN(

    CORE_zgemm(PlasmaNoTrans, PlasmaNoTrans, tempmm, 
	tempnn, tempkn, (dague_complex64_t)-1.000000, 
	C /* A(m1,k) */, ldam, D /* X(k,n1) */, 
	ldxk, (dague_complex64_t)1.000000, E /* Z(m1,n1) */, 
	ldzm );  );

  printlog("thread %d CORE_zgemm(%d, %d, %d)\n"
           "\t(PlasmaNoTrans, PlasmaNoTrans, tempmm, tempnn, tempkn, (dague_complex64_t)-1.000000, A(%d,%d)[%p], ldam, X(%d,%d)[%p], ldxk, (dague_complex64_t)1.000000, Z(%d,%d)[%p], ldzm)\n",
  context->th_id, m1, n1, k, m1, k, C, k, n1, D, m1, n1, E);

END

/*
 * Pseudo-task
 */
zgemm_36_in_A0(m1,n1,k)
  m1 = 0..(descZ.mt-1)
  n1 = 0..(descZ.nt-1)
  k = 0..(descA.nt-1)

  : A(m1,k)

  RW C <- A(m1,k)
       -> C zgemm_36(m1,n1,k)
BODY
/* nothing */
END


